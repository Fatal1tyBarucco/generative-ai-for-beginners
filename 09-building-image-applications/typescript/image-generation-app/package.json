{
  "name": "image-generation-app",
  "version": "1.0.0",
  "description": "a simple application how to manage text completions with Azure OpenAI in Typescript",
  "main": "main.js",
  "scripts": {
    "build": "rimraf build && tsc",
    "preserve": "npm run build",
    "start": "nodemon"
  },
  "keywords": [
    "typescript",
    "azure",
    "openai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.13.14",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@azure/openai": "^2.0.0",
    "dotenv": "^16.3.1",
    "rimraf": "^5.0.7"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "npx ts-node ./src/main.ts"
  }
}